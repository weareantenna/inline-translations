{
    "name": "antenna/inline-translations",
    "description": "Inline translation package for laravel with plugins for blade & vue",
    "keywords": [
        "translations",
        "laravel",
        "vue"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antenna",
            "email": "info@weareantenna.be"
        }
    ],
    "require": {
        "php" : "^8.0 || ^8.1",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/translation": "^7.10 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/routing": "^7.10 || ^8.0 || ^9.0 || ^10.0",
        "league/flysystem": "^3.0",
        "ext-json": "*",
        "league/csv": "^9.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.1",
        "doctrine/coding-standard": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpstan/phpstan": "^1.4",
        "illuminate/http": "^7.10 || ^8.0 || ^9.0 || ^10.0",
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench": "^5.2 || ^6.0 || ^7.4",
        "league/flysystem-memory": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Antenna\\InlineTranslations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antenna\\InlineTranslations\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Antenna\\InlineTranslations\\InlineTranslationsServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
